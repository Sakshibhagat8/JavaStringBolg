// Using for loop
class Main{
    public static void main(String args[])
    {
        String str="sakshooi";
        int len=str.length();
        char[] ch=str.toCharArray();
        for(int i=0;i<len;i++)
        {
            for(int j=i+1;j<len;j++)
            {
                if(ch[i]==ch[j])
                {
                    System.out.println("Duplicate character in string is "+ch[j]);
                    break;
                }
            }
        }
    }
}
